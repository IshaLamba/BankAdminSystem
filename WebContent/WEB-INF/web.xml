<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FinalProject</display-name>
  <welcome-file-list>
    <welcome-file>HOME.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>balance</servlet-name>
    <servlet-class>com.Project.Balance</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>balance</servlet-name>
    <url-pattern>/balance</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>withdraw</servlet-name>
    <servlet-class>com.Project.Withdraw</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>withdraw</servlet-name>
    <url-pattern>/withdraw</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deposit</servlet-name>
    <servlet-class>com.Project.Deposit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deposit</servlet-name>
    <url-pattern>/deposit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>wConfirm</servlet-name>
    <servlet-class>com.Project.Withdraw1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>wConfirm</servlet-name>
    <url-pattern>/wConfirm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>dConfirm</servlet-name>
    <servlet-class>com.Project.Deposit1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dConfirm</servlet-name>
    <url-pattern>/dConfirm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>eLogin</servlet-name>
    <servlet-class>com.Project.Elogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>eLogin</servlet-name>
    <url-pattern>/eLogin</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>withdrawFilter</filter-name>
    <filter-class>com.Project.WithdrawFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>withdrawFilter</filter-name>
    <servlet-name>withdraw</servlet-name>
  </filter-mapping>
  <filter>
    <filter-name>depositFilter</filter-name>
    <filter-class>com.Project.DepositFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>depositFilter</filter-name>
    <servlet-name>deposit</servlet-name>
  </filter-mapping>
  <context-param> 
<description>Location to store uploaded file</description> 
<param-name>file-upload</param-name> 
<param-value> 
C:\work</param-value> 
</context-param>
</web-app>